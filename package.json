{
  "name": "rss",
  "version": "1.0.0",
  "description": "Starter package with Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:cannyedge34/rss.git",
  "scripts": {
    "clean": "rimraf dist",
    "build":
      "rimraf dist/ && NODE_ENV=production webpack --config webpack.prod.config.js --progress --hide-modules",
    "start":
      "NODE_ENV=development webpack-dev-server --config webpack.dev.config.js --open --inline --hot",
    "test":
      "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "deploy": "npm run build && git subtree push --prefix dist origin gh-pages"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^4.1.2",
    "chai-jquery": "^2.0.0",
    "compression-webpack-plugin": "^1.1.2",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "jsdom": "^11.5.1",
    "mocha": "^4.0.1",
    "postcss-loader": "^2.0.9",
    "preload-webpack-plugin": "^2.0.0",
    "react-addons-test-utils": "^15.6.2",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-ext-html-webpack-plugin": "^3.4.5",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-preset-stage-1": "^6.1.18",
    "materialize-css": "^0.100.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-materialize": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-thunk": "^2.2.0",
    "youtube-api-search": "0.0.5"
  }
}
